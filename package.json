{
  "name": "@zanejs/lint-config",
  "version": "1.0.1",
  "description": "A powerful lint configuration collection that provides out-of-the-box code quality solutions for modern frontend projects.",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc -b ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile"
  },
  "files": [
    "dist",
    "README.md",
    "README.zh-CN.md"
  ],
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@manypkg/get-packages": "^3.1.0",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-turbo": "^2.7.4",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "find-up": "^8.0.0",
    "globals": "^17.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript": "^5.9.3",
    "npm-run-all": "^4.1.5",
    "vue-eslint-parser": "^10.2.0"
  }
}
